{
  "name": "iamla",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "description": "Application de gestion des visites m√©dicales pour RycaPharma",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --verbose --detectOpenHandles",
    "build": "npm install --production",
    "clean": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "format": "prettier --write src/**/*.{js,ts,tsx}",
    "validate": "npm run lint && npm run test"
  },
  "config": {
    "port": "8100"
  },
  "keywords": [
    "medical",
    "visits",
    "tracking",
    "pharma",
    "rycapharma"
  ],
  "author": "RycaPharma",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.16.14",
    "@mui/x-date-pickers": "^6.20.2",
    "@reduxjs/toolkit": "^2.5.1",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "framer-motion": "^12.4.5",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mysql2": "^3.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.5.1",
    "supertest": "^6.3.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
